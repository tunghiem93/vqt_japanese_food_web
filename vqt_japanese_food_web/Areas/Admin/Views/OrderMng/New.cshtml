@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@model JapaneseFood.Model.Order.OrderDto
<div class="menu-management">
    @await Html.PartialAsync("_breadcrumb", new Dictionary<string, string> { { "A", "Home" }, { "B", "Order Management" }, { "C", "Add new" } }, new ViewDataDictionary(ViewData) { { "mHref", Url.Action("Index", "OrderMng", new { area = "Admin" }) }, { "mShowButton", "false" } })

    @using (Html.BeginForm("New", "OrderMng", FormMethod.Post, new { @id = "form-create", @class = "form-horizontal needs-validation", @enctype = "multipart/form-data" }))
    {
        @await Html.PartialAsync("_Form", Model)
    }
</div>
