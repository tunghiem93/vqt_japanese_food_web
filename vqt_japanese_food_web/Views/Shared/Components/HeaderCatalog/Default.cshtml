@model List<JapaneseFood.Model.Catalog.CatalogDto>

<ul class="navbar-nav">
    @foreach (var item in Model)
    {
        <li class="nav-item">
            <a class="nav-link"
               asp-controller="Catalog"
               asp-action="Index"
               asp-route-id="@item.Id">
                @item.Name
            </a>
        </li>
    }
</ul>