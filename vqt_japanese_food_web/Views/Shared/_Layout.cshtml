<!doctype html>
<html class="no-js" lang="vi">
<head>
    <link rel="shortcut icon" href="@Url.Content("~/logo.png")" type="image/png" />
    <meta http-equiv="content-type" content="text/html" />
    <meta charset="utf-8" />
    <title>food vqt</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="@Url.Content("~/Content/web/option_selection.js")"></script>
    <script src="@Url.Content("~/Content/web/api.jquery.js")"></script>
    <script src="@Url.Content("~/Content/web/lazyload.js")"></script>
    <script src="@Url.Content("~/Content/web/pluginhead.js")"></script>
    <link href="@Url.Content("~/Content/web/plugins-css.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/web/styles.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/web/responsive.css")" rel="stylesheet" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Food - VQT" />
    <meta property="og:description" content="Food VQT." />
    <meta property="og:url" content="https://VQT.com/" />
    <meta property="og:site_name" content="VQT" />

    <script>
        var formatMoney = '{{amount}}₫';
        window.shop = {
            template: "index",
            moneyFormat: "{{amount}}₫",
            limitCartTitle: "Giỏ hàng chỉ được mua tối đa 2 sản phẩm cho mỗi lần thanh toán",
            limitCartTitleCombo: "Giỏ hàng chỉ được mua tối đa 2 sản phẩm cho mỗi lần thanh toán",
            limitCartTitlePrice: "Đối với sản phẩm sale, giỏ hàng tối đa 1 sản phẩm",
            limitCartTitleOnlyOnline: "Giỏ hàng chỉ được mua tối đa 1 sản phẩm cho mỗi lần thanh toán",
            functionJuly: "false",
            limitCart: 2,
            limitVariant: 5,
            limitStatusInProduct: 11
        }
    </script>
</head>
<body id="vqt-template" class="index">
    <div class="main-body">
        @await Html.PartialAsync("_mheader")
        <div>
            @await Html.PartialAsync("_header")

            @RenderBody()

            @await Html.PartialAsync("_footer") 
        </div>
    </div>

    <div id="site-nav--mobile" class="site-nav style--sidebar">
        <div id="site-cart" class="site-nav-container" tabindex="-1">
            <div class="site-nav-container-last">
                <p class="title">Shopping Cart</p>
                <span class="textCartSide">You currently have <b>0</b> items in your cart.</span>
                <div class="cart-view clearfix">
                    <table id="clone-item-cart" class="table-clone-cart">
                        <tr class="item_2 hidden">
                            <td class="img">
                                <a href="" title=""><img src="" alt="" /></a>
                                <span class="hidden labelGiftInCart">GIFT ITEM</span>
                            </td>
                            <td>
                                <a class="pro-title-view" href="" title=""></a>
                                <span class="pro-price-view"></span>
                                <del class="pro-price-del-view"></del>
                                <span class="variant"></span>
                                <span class="pro-quantity-view"></span>
                                <span class="remove_link remove-cart removePro"></span>
                            </td>
                        </tr>
                    </table>
                    <table id="cart-view">
                        <tr>
                            <td>No items yet</td>
                        </tr>
                    </table>
                    <span class="line"></span>
                    <table class="table-total">
                        <tr>
                            <td class="text-left"><b>SUBTOTAL:</b></td>
                            <td class="text-right" id="total-view-cart">0₫</td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <a href="@Url.Action("CheckOut", "Cart")" class="linktocheckout button dark">Proceed to Checkout</a>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <a href="@Url.Action("Index", "Cart")" class="linktocart button dark">View Cart Details <i class="fa fa-arrow-right"></i></a>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>

        <div id="site-search" class="site-nav-container" tabindex="-1">
            <div class="site-nav-container-last">
                <p class="title">Search</p>
                <div class="search-box wpo-wrapper-search">
                    <form action="" class="searchform searchform-categoris ultimate-search navbar-form">
                        <div class="wpo-search-inner">
                            <input type="hidden" name="type" value="product" />
                            <input required id="inputSearchAuto" name="q" maxlength="40" autocomplete="off" class="searchinput input-search search-input" type="text" size="20" placeholder="Search products...">
                        </div>
                        <button type="submit" class="btn-search btn" id="search-header-btn">
                            <svg version="1.1" class="svg search" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 24 27" style="enable-background:new 0 0 24 27;" xml:space="preserve">
                            <path d="M10,2C4.5,2,0,6.5,0,12s4.5,10,10,10s10-4.5,10-10S15.5,2,10,2z M10,19c-3.9,0-7-3.1-7-7s3.1-7,7-7s7,3.1,7,7S13.9,19,10,19z"></path>
                            <rect x="17" y="17" transform="matrix(0.7071 -0.7071 0.7071 0.7071 -9.2844 19.5856)" width="4" height="8"></rect>
                        </svg>
                        </button>
                    </form>
                    <div id="ajaxSearchResults" class="smart-search-wrapper ajaxSearchResults" style="display: none">
                        <div class="resultsContent"></div>
                    </div>
                </div>
            </div>
        </div>

        <button id="site-close-handle" class="site-close-handle" aria-label="Close" title="Close">
            <img src="//file.hstatic.net/1000360430/file/iconclose_32c1d6a3da3545278283605bbfca0b0b.png" alt="Close" />
        </button>
    </div>

    <div id="site-overlay" class="site-overlay"></div>
    <div id="quickview-cart" class="modal fade" role="dialog">
        <div id="quickview-cart-desktop" class="clearfix">
        </div>
    </div>


    <script src="@Url.Content("~/Content/web/plugins.js")"></script>
    <script src="@Url.Content("~/Content/web/scripts.js")"></script>
    <script src="@Url.Content("~/Content/web/scripts_main.js")"></script>
    <script src="@Url.Content("~/Content/web/countdown.js")"></script>
    <script src='https://file.hstatic.net/1000003969/file/insider-sw-sdk_02bfc88d694144209235899c674c0668.js' type='text/javascript'></script>

    <script>
        setTimeout(function () {
            animation_check();
        }, 100);
        function animation_check() {
            var scrollTop = $(window).scrollTop() - 300;
            $('.animation-tran').each(function () {
                if ($(this).offset().top < scrollTop + $(window).height()) {
                    $(this).addClass('active');
                }
            })
        }
        $(window).scroll(function () {
            animation_check();
        })
    </script>
    <script>
        (function (h, o, t, j, a, r) {
            h.hj = h.hj || function () { (h.hj.q = h.hj.q || []).push(arguments) };
