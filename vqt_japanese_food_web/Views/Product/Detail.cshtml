                </div>
            </div>
            <div class="hidden images">
                @if (Model.Images != null && Model.Images.Any())
                {
                    foreach (var image in Model.Images)
                    {
                        <div name-new="_bb03053" class="item itemdelete @image.Color" data-original="@Url.Content(image.ImageURL)" data-option="@image.Color" data-variant="@Model.Code">
                            <a href="@Url.Content(image.ImageURL)" title="Click để xem" data-option="@image.Color" data-image="@Url.Content(image.ImageURL)" data-zoom-image="@Url.Content(image.ImageURL)" rel="lightbox-@image.Color">
                                <img class="removeImgMobile hidden-xs img-fluid img-responsive" src="@Url.Content(image.ImageURL)">
                                <img class="removeImgDesktop visible-xs img-fluid img-responsive" src="@Url.Content(image.ImageURL)">
                                <p class="click-p" href="@Url.Content(image.ImageURL)" data-zoom-image="@Url.Content(image.ImageURL)" rel="lightbox-@image.Color">
                                    <i class="fa fa-search" aria-hidden="true"></i>
                                    Click xem hình lớn hơn
                                </p>
                            </a>
                        </div>
                    }
                }
            </div>
            <div class="hidden thumbnails-hidden">
                @if (Model.Images != null && Model.Images.Any())
                {
                    foreach (var image in Model.Images)
                    {
                        <div class="  thumbnail thumdelete clickItem" data-option="@image.Color" data-zoom="@Url.Content(image.ImageURL)">
                            <img class="img-fluid img-thumbnail" src="@Url.Content(image.ImageURL)">
                        </div>
                    }
                }
            </div>
        </div>
        <script>
         
         var ITEM = null;

         /*--------View Store--------*/
         var owl, soption = {
             items: 1,
             nav: true,
             dots: false,
             navText: ["", ""],
             transitionStyle: "fade",
             singleItem: true,
             afterInit: function (elem) {
             var current = this.currentItem;
             },
             afterMove: function (elem) {
             var current = this.currentItem;
             }
         };
         var switchSlide = function (opt) {
              var imagelast = '';
              if ($("#mainDetailProduct #slide-image").data('owlCarousel')) {
                  owl.data('owlCarousel').destroy();
              }
              $("#mainDetailProduct #slide-image").html('');
              $('#mainDetailProduct #slide-image .item').remove();
              $('#mainDetailProduct .images .item[data-option=' + opt + ']').clone().appendTo('#slide-image');
              $('#mainDetailProduct .thumbnails .row .thumbnail').remove();
              $('#mainDetailProduct .thumbnails .row').html('');
              $('#mainDetailProduct .thumbnails-hidden .thumbnail[data-option=' + opt + ']').clone().appendTo('.thumbnails .row');
              owl = $("#mainDetailProduct #slide-image").owlCarousel(soption);
              $("#mainDetailProduct a[rel^='lightbox']").picbox({}, null, function (el) {
              return (this == el) || ((this.rel.length > 8) && (this.rel == el.rel));
              });
         }
         $(document).on('click', '#mainDetailProduct .thumbnails .thumbnail.clickItem', function () {
             owl.trigger("to.owl.carousel", [$(this).index(), 500, true]);
             $("#mainDetailProduct #slide-image").show();
             $("#mainDetailProduct .hinh360").hide();
             $("#mainDetailProduct .videoProduct").hide();
         });
         $(document).on('click', '#mainDetailProduct .thumbnails .thumbnail', function () {
         $('#mainDetailProduct .thumbnails .thumbnail').removeClass('active');
             $(this).addClass('active');
         });

         $(document).ready(function(){
             $("#mainDetailProduct .color_each .swatch-element label.sd").each(function(){
                 var a = $(this).attr("data-handle");
                 switchSlide(a);
             })
         $('#mainDetailProduct .owlRelate1 .owl-carousel').owlCarousel({
             items:4,
             nav: true,
             dots: true,
             lazyLoad:true,
             touchDrag: true,
             responsive:{
             0:{
             items:2
             },
             767:{
             items:2
             },
             1024:{
             items:4
             }
             }
         });
         $("#mainDetailProduct .tabUlRelate li").click(function(){
             $("#mainDetailProduct .tabUlRelate li").removeClass("active");
             $(this).addClass("active");
             var dataTab = $(this).attr("data-tab");
             $("#mainDetailProduct .tabDetailProduct").hide();
             $("#mainDetailProduct .tabDetailProduct[data-tab="+dataTab+"]").show();
             $('#mainDetailProduct .owlRelate2 .owl-carousel').owlCarousel({
             items:4,
             nav: true,
             dots: true,
             lazyLoad:true,
             touchDrag: true,
             responsive:{
             0:{
             items:2
             },
             767:{
             items:2
             },
             1024:{
             items:4
             }
             }
             });
             HuyHoang.Main.colorVariant();
         });
         $("#mainDetailProduct .swatch .swatch-element input").click(function(){
             $("#mainDetailProduct #slide-image").show();
             $("#mainDetailProduct .hinh360").hide();
             $("#mainDetailProduct .videoProduct").hide();
         });
         $('#mainDetailProduct .color_each input').click(function(){
             var b = $(this).attr("data-vhandle");
             switchSlide(b);
         });
         var checksize = $("#mainDetailProduct .list-productRelated .content-product-list > div").size();
         if (checksize == 0){
         $("#mainDetailProduct .list-productRelated .content-product-list").append("<div class='emptyRelate'>Đang cập nhật sản phẩm!</div>");
         }
         $(document).on('click','#mainDetailProduct .btn--view-more', function (e) {
             e.preventDefault();
             var $this = $(this);
             $this.parents('#product-review').toggleClass('expanded');
             return false;
         });
         $("#mainDetailProduct .product-description .title-bl").click(function(){
             $(this).find(".icon-open").toggleClass("active");
             $(this).next().slideToggle();
         });

         /**** HuyHoang ****/
             $('#add-to-cart').click(function (e) {
                 var colorCount = $('#mainDetailProduct #add-item-form label.sd').length;
                 var sizeCount = $('#mainDetailProduct #add-item-form div[id*=variant-swatch-]').length;
                 var sizeElementCount = $('#mainDetailProduct #add-item-form div[id=variant-swatch-1]').length;
                 if (sizeElementCount == 0) {
                 sizeCount = 1;
                 }
                 if (colorCount == sizeCount) {
                    e.preventDefault();
                    $(this).addClass('clicked_buy');
                    var ProductId = $(this).attr('data-id')
                    var ProductPrice = $("#mainDetailProduct #price-preview").attr("data-price-min");
                    var Qty = parseInt($('#inventory-variant-buyer').val());
                    var ColorCode = $('#mainDetailProduct #add-item-form label.sd').attr('data-color-code')
                    var ColorName = $('#mainDetailProduct #add-item-form label.sd').attr('data-color-name')
                    var Size = $("input[name='option1']:checked").val()
                    if (Size == undefined) {
                        Size = "";
                    }
                    var ProductName = $('#productName').val()
                    var ProductCode = $('#productCode').val()
                    var ImageURL = $('.images .item.' + ColorCode + '').attr('data-original')
                    var Item = {
                        Id : ProductId,
                        Price: ProductPrice,
                        Code: ProductCode,
                        Name: ProductName,
                        Qty: Qty,
                        ColorCode: ColorCode,
                        ColorName: ColorName,
                        Size: Size,
                        ImageURL: ImageURL,
                        LinkURL: '@Url.Action("Detail", "Product", new { q = Model.Alias })'
                    }
                    HuyHoang.addToCard(Item);
                    $("#site-cart-handle").click(); 
             }else{
                swal({
                    type: 'error',
                    text: 'Vui lòng chọn size hoặc màu mà bạn thích. Xin cảm ơn. '
                });
             }
         });
         $('#add-to-cartbottom').click(function(e){
             e.preventDefault();
             $(".modalBuyMobile").removeClass("notShow");
             $(".overModalBuyMobile").removeClass("notShow");
         });
         $(".buyNowModalBuyMobile").click(function(){
          $("#add-to-cart").click();
         });
         $(".buyNowModalBuyMobileOnlyOnline").click(function(){
             $("#add-to-cart-only-online").click();
         });
         $(".overModalBuyMobile").click(function(){
         $(this).addClass("notShow");
             $(".modalBuyMobile").addClass("notShow");
         });
         $(".closeModalBuyMobile").click(function(){
             $(".overModalBuyMobile").addClass("notShow");
             $(".modalBuyMobile").addClass("notShow");
         });
         $(".modalBuyMobile .swatch input").click(function(){
             var attrId = $(this).attr("id");
             var split = attrId.split("-mobile")[0];
             $("#add-item-form .swatch input[id="+split+"]").click();
         });
         $('#quan-input').keyup(function(){
             $('[name="quantity"]').val($(this).val());
         })
         $('[name="quantity"]').on('keyup change', function(){
             $('#quan-input').val($(this).val());
         })
         });
         var check_variant = true;
         var fIndex = false;
         var arraryV = "35-xanh-nhat,35-den,35-trang,36-xanh-nhat,36-den,36-trang,37-xanh-nhat,37-den,37-trang,38-xanh-nhat,38-den,38-trang,39-xanh-nhat,39-den,39-trang";
         
        </script>
        <!--Customize JS-->
        <script type="text/javascript">
            /*** Event Change swatch Size and Color ***/
            jQuery(document).on('click', '#mainDetailProduct .swatch input', function (e) {
                var $this = $(this)
                $this.parent().siblings().find('label').removeClass('sd')
                $this.next().addClass('sd')
            });
        </script>
        
    </main>
